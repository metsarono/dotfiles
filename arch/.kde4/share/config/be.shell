Opacity=98
VisibilityTimeout=200

[BE::Shell]
Panels=SideBar,TopPanel
Theme=Pandora

[Browser]
Icon=web-browser
Mode=0
Service=firefox
Type=Button

[ControlMain]
Applets=mediaToggle,infoToggle
Frozen=True
Id=ControlMain
Position=1
Type=Panel

[DummyClock]
Id=DummyClock
Pattern='<html><div align="left">&nbsp;&nbsp;'dddd'</div><div align="center">'dd. MMMM'</div></html>'
Type=Clock

[FeedPanel]
Applets=RSS
Id=FeedPanel
Length=100
Offset=0
Position=0
Screen=-1
Size=26
Type=Panel
Visible=true

[Footer1]
Applets=Test1
Id=Footer
Length=100
Offset=0
Position=0
Screen=-1
Size=26
Type=Panel
Visible=true

[Footerold]
Applets=Test
Id=Footer
Position=0
Type=Panel
Visible=true

[GlobalMenu]
Type=globalmenu
WindowMenus=true

[Music]
Active=True
FiFo=$HOME/.local/share/skutter/mpris
Lines=1
PermittedCommands=dbus
Type=Label
Wrap=true

[MusicPanel]
Applets=Music
Id=MusicPanel
Position=0
Type=Panel
Visible=true

[Packages]
Active=true
Exec=$HOME/.local/share/be.shell/Scripts/applet up
Lines=1
PermittedCommands=konsole --hold -e sudo pacman -Syu,konsole --hold -e yaourt -Sbu --aur
PollInterval=600000
Type=Label

[Page1]
Applets=ToggleMusic,MusicPanel,ToggleSession,SensorPanel,ToggleStuff,StuffPanel,stretch
Frozen=True
Id=Page1
Position=3
Type=Panel
Visible=true

[Page2]
Applets=DummyClock,ToggleFeed,FeedPanel,stretch
Frozen=True
Id=Page2
Position=3
Type=Panel
Visible=false

[PagesContainer]
Applets=Page1,Page2
Frozen=True
Id=PagesContainer
Position=3
Type=Panel

[RSS]
Active=true
Exec=$HOME/.local/share/be.shell/Scripts/rss2htm  http://www.phoronix.com/rss.php
Lines=25
PollInterval=30000
Type=Label
Wrap=true

[SensorPanel]
Applets=Packages
Id=CalendarPanel
Position=0
Type=Panel
Visible=true

[SessionButton]
FullName=false
Mode=1

[SideBar]
ARGB=True
Applets=ControlMain,PagesContainer,Footer1
AutoHideDelay=1000
AutoHideSensorSize=0
BlurRadius=0
CastShadow=false
Frozen=false
Id=SideBar
Layer=3
Length=98
Offset=2
Opacity=100
Position=3
Screen=1
Size=392
Struts=False
Visible=true

[StuffPanel]
Applets=Weather
Id=StuffPanel
Length=100
Offset=0
Position=0
Screen=-1
Size=26
Type=Panel
Visible=true

[Test1]
Exec=qdbus org.bedevil.Skutter /Control org.bedevil.Skutter.Restart
Label=Refresh
Mode=1
Type=Button

[Testold]
Exec=konsole
Label=Test
Mode=1
Type=Button

[Time]
APM=true
Digits=true
HintRange=true
Span=-360
Start=80
Type=TimeMeter

[ToggleFeed]
DBus=session;org.kde.be.shell;/Shell;org.kde.be.shell;togglePanel;FeedPanel
Label=Feeds
Type=Button

[ToggleMusic]
DBus=session;org.kde.be.shell;/Shell;org.kde.be.shell;togglePanel;MusicPanel
Label=Music
Type=Button

[ToggleSession]
DBus=session;org.kde.be.shell;/Shell;org.kde.be.shell;togglePanel;SensorPanel
