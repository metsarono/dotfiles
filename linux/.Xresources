URxvt*termName:rxvt-unicode-256color
Xft.antialias:  true
!Xft.autohint:   true
Xft.hinting:    true
Xft.hintstyle:  hintslight
!Xft.lcdfilter:  lcdlight
Xft.rgba:       rgb
Xft.dpi:        96

URxvt*transparent:false


URxvt.cursorColor:#505050
*background: #333333
*foreground: #C5C8C6
!black
*color0: #282A2E
!*color8: #373B41
*color8: #505050
! red
*color1: #A54242
*color9: #CC6666
! green
*color2: #8C9440
*color10: #B5BD68
! yellow
*color3: #DE935F
*color11: #F0C674
! blue
*color4: #5F819D
*color12: #81A2BE
! magenta
*color5: #85678F
*color13: #B294BB
! cyan
*color6: #5E8D87
*color14: #8ABEB7
! white
*color7: #707880
*color15: #C5C8C6

!Fonts
!URxvt*font:xft:EnvyCodeR:pixelsize=11:antialias=true:hinting=true,xft:Inconsolata-g:pixelsize=11:antialias=true:hinting=true
!urxvt*font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12
URxvt*font: xft:Terminess Powerline:size=12

!Say no to scrollbars
URxvt*scrollBar:false


! Size of my terminal
URxvt.geometry:90x20+890+630
URxvt.imLocale:en_US.UTF8

URxvt*skipBuiltinGlyphs: true
!
! Amount that I can scroll up
URxvt.saveLines: 12000

! Underline URLs
URxvt.underlineURLs:true

URxvt.iso14755_52:false
URxvt.iso14755:false

! Add color to my!man pages:
URxvt.colorIT: #87af5f
URxvt.colorBD: #d7dfdf
!URxvt.colorUL:

